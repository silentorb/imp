test {

	Crust {
		static crispy:int = 1
		static soft:int = 2
		static burnt:int = 3
		static missing:int = 4
	}

	Pizza {
		toppings:string[]
		crust:Crust
		variation:bool
		static taste:string

		add(topping:string) {
			var x = 0
			if (topping == null)
				return
			else
				x = 1.5

			toppings.push(topping)
			crust = Crust.burnt
		}

		get_eaten():string[] {
			return toppings
		}
	}
}